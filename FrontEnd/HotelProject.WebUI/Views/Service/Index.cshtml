
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

@model List<ResultServiceDto>

<table class="table table-bordered">
    <thead>
        <
        <tr>
            <th>#</th>
            <th>Hizmet Başlığı</th>
            
            <th>Sil</th>
            <th>Güncelle</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ServiceID</td>
                <td>@item.Title</td>
                <td><a href="/Service/DeleteService/@item.ServiceID" class="btn btn-danger">Sil</a></td>
                <td><a href="/Service/UpdateService/@item.ServiceID" class="btn btn-success">Güncelle</a></td>


            </tr>
        }
    </tbody>
</table>
<a href="/Service/AddService/" class="btn btn-dark">Yeni Hizmet Girişi</a>


